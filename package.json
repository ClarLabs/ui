{
	"name": "@clarlabs/ui",
	"version": "0.1.21",
	"description": "A modern UI component library for React",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		},
		"./styles.css": "./dist/styles.css",
		"./dist/ui.css": "./dist/ui.css"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "vite build --config vite.config.lib.ts",
		"dev": "vite build --config vite.config.lib.ts --watch",
		"demo": "vite",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "npm run build",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"keywords": [
		"react",
		"ui",
		"components",
		"design-system",
		"ui-kit"
	],
	"author": "ClarLabs",
	"license": "MIT",
	"peerDependencies": {
		"react": ">=19.2.0",
		"react-dom": ">=19.2.0"
	},
	"devDependencies": {
		"@storybook/addon-docs": "^9.1.10",
		"@storybook/addon-onboarding": "^9.1.10",
		"@storybook/react": "^9.1.10",
		"@storybook/react-vite": "^9.1.10",
		"@types/node": "^24.7.2",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.1",
		"@vitejs/plugin-react": "^5.0.4",
		"glob": "^11.0.3",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"sass": "^1.70.0",
		"storybook": "^9.1.10",
		"tsup": "^8.0.0",
		"typescript": "^5.3.0",
		"vite": "^7.1.9",
		"vite-plugin-dts": "^4.5.4"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ClarLabs/ui"
	},
	"publishConfig": {
		"access": "public"
	}
}
